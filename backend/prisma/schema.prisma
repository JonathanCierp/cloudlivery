generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("MYSQL_DATABASE_URI")
}

model User {
  id Int @id @default(autoincrement())
  google_id String? @unique
  lastname String?
  firstname String?
  civilite String?
  email String @unique
  password String?
  updatedAt DateTime @updatedAt @default(now())
  createdAt DateTime @default(now())

  @@map(name: "users")
}