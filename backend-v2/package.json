{
	"name": "backend-v2",
	"version": "1.0.0",
	"description": "",
	"main": "index.ts",
	"scripts": {
		"test": "env-cmd -f ./config/test.env jest --runInBand",
		"dev": "env-cmd -f ./config/dev.env ts-node-dev --no-notify --respawn --transpileOnly ./index",
		"build": "npm -s run generate && tsc",
		"migrate": "npm run migrate:save && npm run migrate:up",
		"migrate:up": "prisma migrate up --experimental --schema ./src/prisma/datamodel.prisma",
		"migrate:save": "prisma migrate save --experimental --schema ./src/prisma/datamodel.prisma",
		"generate": "npm -s run generate:prisma && npm -s run generate:nexus",
		"generate:prisma": "prisma generate --schema ./src/prisma/datamodel.prisma",
		"generate:nexus": "cross-env NODE_ENV=development ts-node --transpile-only src/schema"
	},
	"dependencies": {
		"@prisma/client": "^2.2.0",
		"@types/nodemailer": "^6.4.0",
		"algoliasearch": "^4.4.0",
		"apify": "^0.21.3",
		"bcryptjs": "^2.4.3",
		"ejs": "^3.1.3",
		"env-cmd": "^10.1.0",
		"graphql-yoga": "^1.18.3",
		"jsonwebtoken": "^8.5.1",
		"nexus": "^0.25.0",
		"nexus-plugin-prisma": "^0.16.1",
		"nodemailer": "^6.4.10",
		"prisma-client-lib": "^1.34.10",
		"puppeteer": "^5.1.0",
		"puppeteer-extra": "^3.1.12",
		"puppeteer-extra-plugin-stealth": "^2.4.13",
		"redis": "^3.0.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.7.0",
		"@babel/core": "^7.7.0",
		"@babel/node": "^7.7.0",
		"@babel/plugin-transform-runtime": "^7.6.2",
		"@babel/plugin-transform-spread": "^7.6.2",
		"@babel/preset-env": "^7.7.1",
		"@babel/preset-flow": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@babel/register": "^7.7.0",
		"@babel/runtime": "^7.7.2",
		"@prisma/cli": "^2.2.0",
		"@types/bcryptjs": "^2.4.2",
		"@types/graphql-iso-date": "^3.4.0",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/redis": "^2.8.24",
		"apollo-boost": "^0.4.9",
		"cross-env": "^7.0.2",
		"cross-fetch": "^3.0.5",
		"graphql": "^15.3.0",
		"graphql-iso-date": "^3.6.1",
		"jest": "^26.1.0",
		"ts-node": "^8.10.2",
		"ts-node-dev": "^1.0.0-pre.50",
		"typescript": "^3.9.6"
	},
	"jest": {
		"globalSetup": "./tests/config/globalSetup.ts",
		"globalTeardown": "./tests/config/globalTeardown.ts"
	},
